  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT TempM101.ParentName, TempM101.StrDate, TempM101.ParentName, TempM101.ParentName, TempM101.AccountReceipt, TempM101.ReceiptNo, TempM101.Fee01

FROM TempM101 
       INNER JOIN Users AS U ON U.UserID = @userID and U.SchoolID = @schoolID
       INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.UserID = @userID
WHERE  TempM101.UserID = @userID and TempM101.SchoolID = @schoolID
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY TempM101.StrDate))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY TempM101.ParentName))
         END
</value>
  </data>
  <data name="sqlDataSource2.SelectCommand" xml:space="preserve">
    <value>SELECT        S.CompanyName, S.CompanyAddress, S.Phone1, S.Phone2, S.MobilePhone, S.Fax, S.TaxOffice, S.TaxNo, S.CompanyEmail, S.WebSite, 
              S.AuthorizedPersonName1, S.AuthorizedPersonName2, S.AuthorizedPersonName3, U.UserPeriod, TempM101Header.DateFrom,TempM101Header.DateTo,
              TempM101Header.StartNumber, TempM101Header.BankName, TempM101Header.HeaderFee01, TempM101Header.HeaderFee02
FROM          SchoolInfo AS S 
              INNER JOIN Users AS U ON U.UserID = @userID and U.SchoolID = @schoolID
              INNER JOIN TempM101Header ON TempM101Header.SchoolID = @schoolID and TempM101Header.UserID = @userID
WHERE         S.SchoolID = @schoolID</value>
  </data>
