  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT    TempM101.StudentID, TempM101.StudentName, TempM101.GenderTypeCategoryID, TempM101.StudentNumber,  TempM101.BondDate, TempM101.DateOfRegistration, 
	  TempM101.ParentName, TempM101.ReceiptNo, TempM101.AccountReceipt, TempM101.ParentMobilePhone, TempM101.HomePhone, TempM101.WorkPhone, TempM101.InWriting,
          Parameter1.CategoryName as GENDER, Parameter2.CategoryName as KinshipCategory, Parameter3.CategoryName as ProfessionCategory,
          TempM101Header.DateFrom, TempM101Header.DateTo
	     
FROM TempM101 
              INNER JOIN Users AS U ON U.UserID = @userID
              JOIN Parameter AS Parameter1 ON TempM101.GenderTypeCategoryID = Parameter1.CategoryID
              LEFT JOIN Parameter as Parameter2 ON TempM101.ReceiptNo = Parameter2.CategoryID 
	      LEFT JOIN Parameter as Parameter3 ON TempM101.AccountReceipt = Parameter3.CategoryID
	      INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.UserID = @userID
WHERE   TempM101.SchoolID = U.SchoolID and TempM101.UserID = @userID
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY TempM101.StudentName))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY TempM101.BondDate))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY TempM101.DateOfRegistration))
            WHEN @OrderOptions04 = 1 THEN (RANK() OVER (ORDER BY TempM101.ParentName))
         END</value>
  </data>
  <data name="sqlDataSource2.SelectCommand" xml:space="preserve">
    <value>SELECT        S.CompanyName, S.CompanyAddress, S.Phone1, S.Phone2, S.MobilePhone, S.Fax, S.TaxOffice, S.TaxNo, S.CompanyEmail, S.WebSite, U.UserPeriod, TempM101Header.DateFrom,TempM101Header.DateTo
FROM          SchoolInfo AS S 
              INNER JOIN Users AS U ON U.UserID = @userID
              INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.UserID = @userID
WHERE         S.SchoolID = U.SchoolID</value>
  </data>
