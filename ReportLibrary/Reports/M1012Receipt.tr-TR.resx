  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT  TempM101.ID, TempM101.CashPayment, TempM101.BondTypeTitle, TempM101.InWriting,  TempM101Header.HeaderFee01, TempM101Header.HeaderFee02, TempM101Header.InWriting AS TxtWriting, TempM101Header.StartNumber, TempM101Header.Total
	     
FROM TempM101 
         INNER JOIN Users AS U ON U.UserID = @userID
         INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.HeaderFee12 = 'Receipt2' and TempM101Header.UserID = @userID
WHERE TempM101.SchoolID = U.SchoolID and TempM101.Status = 'Receipt2' and TempM101.UserID = @userID
ORDER BY TempM101.ID, TempM101.CashPayment





</value>
