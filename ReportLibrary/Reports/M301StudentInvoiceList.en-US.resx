  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT        TempM101.StudentID, TempM101.ClassroomID, TempM101.StudentName, TempM101.IdNumber, TempM101.StudentNumber, TempM101.ReceiptNo,  TempM101.BondDate, TempM101.DateOfRegistration,
              TempM101.Fee01,TempM101.Fee02, TempM101.Fee03,TempM101.WriteDate,
	      Classroom.ClassroomName, Classroom.ClassroomID, TempM101Header.DateFrom, TempM101Header.DateTo, TempM101Header.HeaderFee01, TempM101Header.HeaderFee02,TempM101Header.CompanyName
	     
FROM TempM101 
              INNER JOIN Users AS U ON U.UserID = @userID
	      LEFT JOIN Classroom ON  TempM101.ClassroomID = Classroom.ClassroomID and Classroom.SchoolID = U.SchoolID and Classroom.Period = U.UserPeriod
	      INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.UserID = @userID
WHERE TempM101.SchoolID = U.SchoolID
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY Classroom.SortOrder, Classroom.ClassroomName, TempM101.StudentNumber ,TempM101.StudentSerialNumber))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY TempM101.StudentName))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY ABS(TempM101.StudentNumber)))
            WHEN @OrderOptions04 = 1 THEN (RANK() OVER (ORDER BY TempM101.ReceiptNo))
            WHEN @OrderOptions05 = 1 THEN (RANK() OVER (ORDER BY TempM101.BondDate))           
            WHEN @OrderOptions06 = 1 THEN (RANK() OVER (ORDER BY TempM101.DateOfRegistration))
         END</value>
  </data>
  <data name="sqlDataSource2.SelectCommand" xml:space="preserve">
    <value>SELECT        S.CompanyName, S.CompanyAddress, S.Phone1, S.Phone2, S.MobilePhone, S.Fax, S.TaxOffice, S.TaxNo, S.CompanyEmail, S.WebSite, U.UserPeriod, TempM101Header.DateFrom,TempM101Header.DateTo
FROM          SchoolInfo AS S 
              INNER JOIN Users AS U ON U.UserID = @userID
              INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.UserID = @userID
WHERE         S.SchoolID = U.SchoolID
</value>
  </data>
