  <data name="sqlDataSource2.SelectCommand" xml:space="preserve">
    <value>SELECT        S.CompanyName, S.CompanyAddress, S.Phone1, S.Phone2, S.MobilePhone, S.Fax, S.TaxOffice, S.TaxNo, S.CompanyEmail, S.WebSite, 
              S.AuthorizedPersonName1, S.AuthorizedPersonName2, S.AuthorizedPersonName3, U.UserPeriod, TempM101Header.DateFrom,TempM101Header.DateTo
FROM          SchoolInfo AS S 
              INNER JOIN Users AS U ON U.UserID = @userID 
              INNER JOIN TempM101Header on U.SchoolID = TempM101Header.SchoolID and TempM101Header.UserID = @userID
WHERE         S.SchoolID = U.SchoolID</value>
  </data>
