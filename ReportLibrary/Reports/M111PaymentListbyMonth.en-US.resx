  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT        format(DateOfRegistration, 'yyyy MM'), 
              TempM101.StudentName, TempM101.ParentName, TempM101.Fee01, TempM101.Fee02, TempM101.Fee03, TempM101.Fee04, TempM101.Fee05, TempM101.Fee06, TempM101.Fee07, TempM101.Fee08, TempM101.Fee09, TempM101.Fee10, TempM101.Fee11, TempM101.Fee12,TempM101.PreviousAmount, TempM101.NextAmount,
  	      TempM101.Fee01Collection, TempM101.Fee02Collection, TempM101.Fee03Collection, TempM101.Fee04Collection, TempM101.Fee05Collection, TempM101.Fee06Collection, TempM101.Fee07Collection, TempM101.Fee08Collection, TempM101.Fee10Collection, TempM101.Fee11Collection, TempM101.Fee12Collection,
	      TempM101Header.HeaderFee01, TempM101Header.HeaderFee02, TempM101Header.HeaderFee03, TempM101Header.HeaderFee04, TempM101Header.HeaderFee05, TempM101Header.HeaderFee06, TempM101Header.HeaderFee07, TempM101Header.HeaderFee08, TempM101Header.HeaderFee09, TempM101Header.HeaderFee10, TempM101Header.HeaderFee11, TempM101Header.HeaderFee12,
              TempM101.Fee01Balance, Bank.BankName
FROM TempM101 
              INNER JOIN Users AS U ON U.UserID = @userID
              LEFT JOIN Bank ON Bank.BankID = @bankID
	      INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.SchoolID and TempM101Header.UserID = @userID
WHERE TempM101.SchoolID = U.SchoolID and TempM101.UserID = @userID
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY TempM101.StudentName))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY TempM101.ParentName))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY TempM101.Fee01Balance))
         END</value>
  </data>
  <data name="sqlDataSource2.SelectCommand" xml:space="preserve">
    <value>SELECT        S.CompanyName, S.CompanyAddress, S.Phone1, S.Phone2, S.MobilePhone, S.Fax, S.TaxOffice, S.TaxNo, S.CompanyEmail, S.WebSite, U.UserPeriod
FROM          SchoolInfo AS S 
              INNER JOIN Users AS U ON U.UserID = @userID
WHERE         S.SchoolID = U.SchoolID</value>
  </data>
