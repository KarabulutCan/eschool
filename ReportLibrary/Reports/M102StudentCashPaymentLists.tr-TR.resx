  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>IF (@language = 'tr-TR')
BEGIN
	SELECT    Student.StudentID, Student.StudentSerialNumber, Student.StudentNumber, Student.FirstName, Student.LastName, Student.ClassroomID, Student.IsPension, Student.DateOfRegistration,
                         TempM101.ClassroomName, TempM101.ClassroomID, TempM101.Name, Student.GenderTypeCategoryID, 
                         Parameter1.CategoryName, Parameter1.CategoryName as GENDER, Parameter2.CategoryName as REGISTRATION, Parameter3.CategoryName as STATU,
                         TempM101.CashPayment, T.DateFrom,T.DateTo, T.StartNumber
	FROM            TempM101 
                        INNER JOIN Student ON Student.StudentID = TempM101.StudentID and Student.SchoolID = TempM101.SchoolID
                        INNER JOIN Users AS U ON U.UserID = @userID
                        JOIN Parameter AS Parameter1 ON Student.GenderTypeCategoryID = Parameter1.CategoryID
  		        JOIN Parameter as Parameter2 ON Student.RegistrationTypeCategoryID = Parameter2.CategoryID
			JOIN Parameter as Parameter3 ON Student.StatuCategoryID = Parameter3.CategoryID
                        INNER JOIN TempM101Header AS T ON T.SchoolID = @schoolID  and T.UserID = @userID
	WHERE         Student.SchoolID = @schoolID and (T.DateFrom IS NULL or Student.DateOfRegistration BETWEEN T.DateFrom AND T.DateTo) and
          	     (Student.RegistrationTypeCategoryID IN (@RegistrationType) and Student.StatuCategoryID IN (@StatuCategory)) and
          	    (@payers = 1 AND TempM101.CashPayment &gt; 0) or (@nonPayers = 1 AND TempM101.CashPayment = 0)  and TempM101.UserID = @userID
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY TempM101.ReceiptNo, TempM101.ClassroomName, Student.StudentNumber ,Student.StudentSerialNumber))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY Student.FirstName))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY Student.DateOfRegistration))
            WHEN @OrderOptions04 = 1 THEN (RANK() OVER (ORDER BY Student.StudentSerialNumber))
            WHEN @OrderOptions05 = 1 THEN (RANK() OVER (ORDER BY ABS(Student.StudentNumber)))
            WHEN @OrderOptions06 = 1 THEN (RANK() OVER (ORDER BY TempM101.CashPayment))
         END
END

ELSE
BEGIN
	SELECT    Student.StudentID, Student.StudentSerialNumber, Student.StudentNumber, Student.FirstName, Student.LastName, Student.ClassroomID, Student.IsPension, Student.DateOfRegistration,
                         TempM101.ClassroomName, TempM101.ClassroomID, TempM101.Name, Student.GenderTypeCategoryID, 
                         Parameter1.Language1 as CategoryName, Parameter1.Language1 as GENDER, Parameter2.Language1 as REGISTRATION, Parameter3.Language1 as STATU,
                         TempM101.CashPayment, T.DateFrom,T.DateTo, T.StartNumber
	FROM            TempM101 
                        INNER JOIN Student ON Student.StudentID = TempM101.StudentID and Student.SchoolID = TempM101.SchoolID
                        INNER JOIN Users AS U ON U.UserID = @userID
                        JOIN Parameter AS Parameter1 ON Student.GenderTypeCategoryID = Parameter1.CategoryID
  		        JOIN Parameter as Parameter2 ON Student.RegistrationTypeCategoryID = Parameter2.CategoryID
			JOIN Parameter as Parameter3 ON Student.StatuCategoryID = Parameter3.CategoryID
                        INNER JOIN TempM101Header AS T ON T.SchoolID = @schoolID  and T.UserID = @userID
	WHERE         Student.SchoolID = @schoolID and (T.DateFrom IS NULL or Student.DateOfRegistration BETWEEN T.DateFrom AND T.DateTo) and
          	     (Student.RegistrationTypeCategoryID IN (@RegistrationType) and Student.StatuCategoryID IN (@StatuCategory)) and
          	    (@payers = 1 AND TempM101.CashPayment &gt; 0) or (@nonPayers = 1 AND TempM101.CashPayment = 0)  and TempM101.UserID = @userID
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY TempM101.ReceiptNo, TempM101.ClassroomName, Student.StudentNumber ,Student.StudentSerialNumber))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY Student.FirstName))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY Student.DateOfRegistration))
            WHEN @OrderOptions04 = 1 THEN (RANK() OVER (ORDER BY Student.StudentSerialNumber))
            WHEN @OrderOptions05 = 1 THEN (RANK() OVER (ORDER BY ABS(Student.StudentNumber)))
            WHEN @OrderOptions06 = 1 THEN (RANK() OVER (ORDER BY TempM101.CashPayment))
         END
END</value>
