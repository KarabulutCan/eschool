  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>select 
  max(ClassroomName) as classroomName,
  --count(T.SortOrder),
  count(case when p.CategoryName='Erkek' then 1 end) as male_cnt,
  count(case when p.CategoryName='Kız'   then 1 end) as female_cnt,
  count(*) as total_cnt
from TempM101 T
	INNER JOIN Users AS U ON U.UserID = @userID
	INNER JOIN Student S on S.StudentID = T.StudentID and S.SchoolID = T.SchoolID
	INNER JOIN Parameter p ON p.CategoryID = T.GenderTypeCategoryID 
WHERE T.SchoolID = S.SchoolID  and T.UserID = @userID and S.RegistrationTypeCategoryID IN (@RegistrationType) and S.StatuCategoryID IN (@StatuCategory)
group by classroomName
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY classroomName))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY count(*)))
         END
</value>
  </data>
  <data name="sqlDataSource5.SelectCommand" xml:space="preserve">
    <value>IF (@language = 'tr-TR')
BEGIN
	SELECT CategoryID, CategoryName as Language, CategorySubID
	FROM Parameter
	WHERE CategorySubID = @statuCategorySubID
END

ELSE
BEGIN
	SELECT CategoryID, Language1 as Language, CategorySubID
	FROM Parameter
	WHERE CategorySubID = @statuCategorySubID
END</value>
  </data>
