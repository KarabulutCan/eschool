  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT        TempM101.StudentID, TempM101.ClassroomID, TempM101.StudentName, TempM101.ParentName, TempM101.DateOfRegistration, TempM101.TypeAndNo, TempM101.ReceiptNo, TempM101.AccountReceipt, TempM101.CashPayment, TempM101.Fee01,
	      Classroom.ClassroomName, Classroom.ClassroomID, TempM101Header.DateFrom, TempM101Header.DateTo, TempM101.SchoolNumber
	     
FROM TempM101 
		INNER JOIN Users AS U ON U.UserID = @userID
		INNER JOIN  Classroom ON TempM101.ClassroomID = Classroom.ClassroomID and Classroom.Period = U.UserPeriod
 	        INNER JOIN TempM101Header ON TempM101Header.SchoolID = U.schoolID and TempM101Header.UserID = @userID
WHERE TempM101.SchoolID = U.SchoolID and TempM101.UserID = @userID
ORDER BY  CASE
            WHEN @OrderOptions00 = 1 THEN (RANK() OVER (ORDER BY TempM101.SchoolNumber))
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY Classroom.SortOrder, Classroom.ClassroomName, TempM101.StudentNumber ,TempM101.StudentSerialNumber))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY TempM101.StudentName))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY TempM101.ParentName))
            WHEN @OrderOptions04 = 1 THEN (RANK() OVER (ORDER BY TempM101.DateOfRegistration))
            WHEN @OrderOptions05 = 1 THEN (RANK() OVER (ORDER BY TempM101.TypeAndNo))
            WHEN @OrderOptions06 = 1 THEN (RANK() OVER (ORDER BY TempM101.ReceiptNo))
            WHEN @OrderOptions07 = 1 THEN (RANK() OVER (ORDER BY TempM101.AccountReceipt))
            WHEN @OrderOptions08 = 1 THEN (RANK() OVER (ORDER BY TempM101.Fee01))
         END</value>
  </data>
  <data name="sqlDataSource3.SelectCommand" xml:space="preserve">
    <value>SELECT        ClassroomName, ClassroomID
FROM            Classroom
              INNER JOIN Users AS U ON U.UserID = @userID
WHERE Classroom.SchoolID = U.SchoolID and Classroom.Period = U.UserPeriod
ORDER BY Classroom.SortOrder, Classroom.ClassroomName ASC</value>
  </data>
