  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT  S.StudentID, S.FirstName, S.Lastname, (S.FirstName + ' ' + S.Lastname) AS StudentName, S.ParentName, S.ClassroomID, S.StudentSerialNumber, S.StudentNumber,  S.IsPension, T.ClassroomName, T.ClassroomID
FROM    TempM101 AS T 
        INNER JOIN Student AS S ON T.StudentID = S.StudentID
	INNER JOIN Users AS U ON U.UserID = @userID
        INNER JOIN StudentInvoice I on I.StudentID = S.StudentID and I.SchoolID = U.SchoolID and I.Period = U.UserPeriod
WHERE T.SchoolID = @schoolID and T.UserID = @userID and            
( (S.FirstName + ' ' + S.Lastname ) LIKE ('%' + @StudentName + '%') and (@StudentName) &lt;&gt; ' ')
or
( T.ClassroomID = @Classroom and (U.UserPeriod = @period) and (@StudentName) = ' ')

</value>
