  <data name="sqlDataSource5.SelectCommand" xml:space="preserve">
    <value>SELECT        S.StudentID, S.FirstName, S.Lastname, (S.FirstName + ' ' + S.Lastname) AS StudentName, S.StudentNumber, I.StudentInvoiceID, I.StudentID, I.InvoiceSerialNo, I.InvoiceDate, I.InvoiceCutDate, I.DUnitPrice, I.DDiscount, I.DTax, I.DAmount, I.InvoiceStatus,
              I.InvoiceStatus, I.IsActive, T.ClassroomName
FROM    TempM101 AS T 
        INNER JOIN Student AS S ON T.StudentID = S.StudentID
              INNER JOIN Users AS U ON U.UserID = @userID
              INNER JOIN StudentInvoice I on I.StudentID = S.StudentID and I.SchoolID = U.SchoolID and I.Period = U.UserPeriod                        
WHERE       I.StudentID = @StudentID and S.SchoolID = U.SchoolID  and T.UserID = @userID
</value>
