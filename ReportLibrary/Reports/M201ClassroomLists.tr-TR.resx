  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT        Student.StudentID, Student.StudentSerialNumber, Student.StudentNumber, Student.FirstName, Student.LastName, Student.ClassroomID, Student.IsPension, T.ClassroomName, T.ClassroomID, 
                         T.Name, Student.GenderTypeCategoryID, Parameter.CategoryName, Parameter.CategoryID
FROM          TempM101 AS T 
                        INNER JOIN Student ON Student.StudentID = T.StudentID
                        INNER JOIN Users AS U ON U.UserID = @userID
                        INNER JOIN Parameter ON Student.GenderTypeCategoryID = Parameter.CategoryID
WHERE         U.UserPeriod = @period and Student.SchoolID = T.SchoolID and Student.IsActive = 1 and T.UserID = @userID and T.ClassroomID IN (@classroom) 
	ORDER BY  CASE
            WHEN @OrderOptions01 = 1 THEN (RANK() OVER (ORDER BY Student.StudentSerialNumber))
            WHEN @OrderOptions02 = 1 THEN (RANK() OVER (ORDER BY Student.StudentNumber))
            WHEN @OrderOptions03 = 1 THEN (RANK() OVER (ORDER BY Student.FirstName))
         END</value>
  </data>
  <data name="sqlDataSource2.SelectCommand" xml:space="preserve">
    <value>SELECT        S.CompanyName, S.CompanyAddress, S.Phone1, S.Phone2, S.MobilePhone, S.Fax, S.TaxOffice, S.TaxNo, S.CompanyEmail, S.WebSite, U.UserPeriod
FROM          SchoolInfo AS S 
              INNER JOIN Users AS U ON U.UserID = @userID
WHERE         S.SchoolID = U.SchoolID</value>
  </data>
