                                    function okClick() {
                                        var grid = $("#gridDetailed").data("kendoGrid");
                                        grid.saveChanges();
                                        var pressbuttonindex = $("#InstallmentButtons").data("kendoButtonGroup").current().index();
                                        CollepsDetailed();
                                        ExpandInstallment();
