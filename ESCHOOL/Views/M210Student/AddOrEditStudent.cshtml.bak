                                        $("#div2").hide(500);
                                        $("#div3").show(500);

                                        var unitPrice = $("#gridFees").data().kendoGrid.dataSource.aggregates().unitPrice.sum;
                                        var discount = $("#gridFees").data().kendoGrid.dataSource.aggregates().discount.sum;
                                        var total = unitPrice - discount;

