    <SqlDataSource QueryDesignerState="" ParameterValues="{&quot;@period&quot;:&quot;2024-2025&quot;,&quot;@schoolID&quot;:&quot;2&quot;,&quot;@studentID&quot;:&quot;1&quot;}" ConnectionString="Data Source=.;Initial Catalog=10001;Integrated Security=True" ProviderName="System.Data.SqlClient" SelectCommand="SELECT        S.IdNumber, S.FirstName, S.Lastname, (S.FirstName + ' ' + S.Lastname) AS StudentName, S.ParentName, Classroom.ClassroomName, S.DateOfRegistration, SA.Address1, SA.ZipCode1,&#xD;&#xA;&#x9;&#x9;&#x9;  P0.CategoryName AS CityParameterID1, P00.CategoryName AS TownParameterID1, SC.CompanyName&#xD;&#xA;FROM       Student S&#xD;&#xA;                        LEFT JOIN StudentPeriods AS SP ON S.StudentID = SP.StudentID and SP.Period = @period&#xD;&#xA;                        INNER JOIN Classroom ON (S.ClassroomID  = Classroom.ClassroomID and S.ClassroomID &gt; 0 and Classroom.Period = @period)&#xD;&#xA;                        LEFT JOIN StudentAddress SA on SA.StudentID = S.StudentID&#xD;&#xA;                        LEFT JOIN Parameter AS P0 ON SA.CityParameterID1 = P0.CategoryID&#xD;&#xA;                        LEFT JOIN Parameter AS P00 ON SA.TownParameterID1 = P00.CategoryID&#xD;&#xA;                        LEFT JOIN SchoolInfo AS SC ON SC.SchoolID = @schoolID&#xD;&#xA;&#xD;&#xA;WHERE S.StudentID = @studentID and Classroom.Period = @period&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" Name="sqlDataSource1">
