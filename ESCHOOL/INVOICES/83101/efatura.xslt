                                    <xsl:if test="$DocumentCurrency = 'TL'">
                                      <xsl:text>TL</xsl:text>
                                    </xsl:if>
                                    <xsl:if test="$DocumentCurrency != 'TL'">
                                      <xsl:value-of select="$DocumentCurrency"/>
                                    </xsl:if>
                                  </td>
                                </tr>
